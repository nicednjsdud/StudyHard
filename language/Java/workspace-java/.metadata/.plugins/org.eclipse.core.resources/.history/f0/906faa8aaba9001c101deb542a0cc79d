package kr.co.ezenac.loop2;

import java.util.Scanner;

/*
 *  =====================
 *  1.증속   2.감속   3.중지
 *  =====================
 *  입력 : 1 
 *  
 *  현재 속도 : 1
 *  
 *  
 *  
 */
public class WhileTest2 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		while(true) {
			System.out.println("=====================");
			System.out.println("1.증속   2.감속   3.중지");
			System.out.print();
		}

	}

}
