package kr.co.ezenac.loop;

// 2의 배수 출력

public class ForExtension {

	public static void main(String[] args) {
		
		int i = 0;
		for(; i<10; i++) {			// 초기식 생략
			if (i%2 ==0)			// 2의 배수
				System.out.print(i + " ");
		}
		
		i =0;
		for( ; i<10; ) {			// 초기식, 증감식 생략
			if(i%2==0)
				System.out.println(i + " ");
			i++;
		}
	}

}
